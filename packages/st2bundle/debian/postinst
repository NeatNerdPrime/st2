#!/bin/sh
# postinst script for st2bundle
#
# see: dh_installdeb(1)

set -e
USER=st2
ST2_OWNED="/etc/st2/htpasswd /var/log/st2"

# Mind that ST2_OWNED paths have to exist, i.e should be installed by package
echo $ST2_OWNED | xargs -n1 chown $USER:$USER

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.

#DEBHELPER#

exit 0
